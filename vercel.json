{
  "rewrites": [
    {
      "source": "/",
      "destination": "/blank.html"
    },
    {
      "source": "/palmsprings",
      "destination": "/index.html"
    },
    {
      "source": "/palmsprings/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/fonts/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}

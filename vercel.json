{
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "^/fonts/(.*)",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "^/$",
      "status": 404
    },
    {
      "src": "^/palmsprings/?$",
      "dest": "/index.html"
    },
    {
      "src": "^/palmsprings/.*",
      "dest": "/index.html"
    }
  ]
}
